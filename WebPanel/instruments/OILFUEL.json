{ 
    "src": "instruments/OILFUEL.svg",
    "animations": [
        {
            "element": "#fuelleft",
            "type":    "transform",
            "transforms": [
                {
                    "type": "rotate",
                    "a": {
                        "property": "fuelleft",
                        "scale": 2.1
                    },
                    "x": 125,
                    "y": 830
                }
            ]
        },
        {
            "element": "#fuelright",
            "type":    "transform",
            "transforms": [
                {
                    "type": "rotate",
                    "a": {
                        "property": "fuelright",
                        "scale": 2.1
                    },
                    "x": 380,
                    "y": 830
                }
            ]
        },
        {
            "element": "#oilpress",
            "type":    "transform",
            "transforms": [        
                {
                    "type": "rotate",
                    "a": {
                        "property": "oilpress",
                        "interpolation": "instruments/OilPressInterpolation.xml"
                    },
                    "x": 125,
                    "y": 1030
                }
            ]
        },
        {
            "element": "#oiltemp",
            "type":    "transform",
            "transforms": [
                {
                    "type": "rotate",
                    "a": {
                        "property": "oiltemp",
                        "interpolation": "instruments/OilTempInterpolation.xml"
                    },
                    "x": 380,
                    "y": 1030
                }
            ]
        }
    ]
}
